generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model History {
  id         String   @id @default(cuid())
  gameId     String   @unique
  winner     String?
  board      String
  players    String
  created_at DateTime @default(now())
  status     String   @default("in progress")
}
